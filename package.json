{
  "name": "subagent",
  "version": "0.3.5",
  "description": "CLI tool for managing workspace agents across VS Code",
  "type": "module",
  "bin": {
    "subagent": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && shx cp -r src/vscode/subagent_template dist/vscode/",
    "dev": "tsx src/cli.ts",
    "start": "node --enable-source-maps dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "keywords": [
    "cli",
    "agent",
    "workspace",
    "vscode",
    "automation",
    "parallel",
    "ai",
    "copilot",
    "subagent"
  ],
  "author": "EntityProcess",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EntityProcess/subagent.git"
  },
  "bugs": {
    "url": "https://github.com/EntityProcess/subagent/issues"
  },
  "homepage": "https://github.com/EntityProcess/subagent#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "json5": "^2.2.3",
    "pino": "^9.5.0",
    "rotating-file-stream": "^3.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^2.1.8",
    "shx": "^0.3.4",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  }
}
